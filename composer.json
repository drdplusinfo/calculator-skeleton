{
  "name": "drdplus/calculator-skeleton",
  "license": "MIT",
  "type": "library",
  "description": "Style and functionality helper of a DrD+ calculator",
  "autoload": {
    "psr-4": {
      "DrdPlus\\": "DrdPlus/"
    }
  },
  "require": {
    "drdplus/rules-skeleton": "~1.1",
    "granam/strict-object": "^3.0",
    "granam/string": "^3.1",
    "granam/scalar": "^4.0",
    "granam/exceptions-hierarchy": "^4.0",
    "granam/integer": "^6.1",
    "granam/tools": "^3.0",
    "php": ">=7.1"
  },
  "require-dev": {
    "composer/composer": "^1.8",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~7.0",
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "sort-packages": true
  }
}
